  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "dev:advanced": "npx vite --config config/build/vite.config.ts",
    "build": "npm run security:check && npx vite build --config config/build/vite.config.ts",
    "build:dev": "npx vite build --mode development --config config/build/vite.config.ts",
    "build:unsafe": "NODE_ENV=production npx vite build",
    "build:test": "NODE_ENV=production npx vite build --config config/build/vite.config.ts",
    "build:vercel": "vite build",
    "lint": "npx eslint . --config config/quality/eslint.config.js",
    "preview": "npx vite preview --config config/build/vite.config.ts",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "npx vitest --config config/build/vitest.config.ts",
    "test:watch": "npx vitest --watch --config config/build/vitest.config.ts",
    "test:coverage": "npx vitest --coverage --config config/build/vitest.config.ts",
    "test:ui": "npx vitest --ui --config config/build/vitest.config.ts",
    "test:integration": "npx vitest --config config/build/vitest.config.ts src/**/*.integration.test.tsx",
    "cypress": "cypress open --config-file cypress/cypress.config.ts",
    "cypress:run": "cypress run --config-file cypress/cypress.config.ts",
    "e2e": "npm run preview & sleep 3 && cypress run --config-file cypress/cypress.config.ts; pkill -f 'vite preview'",
    "e2e:open": "npm run preview & sleep 3 && cypress open --config-file cypress/cypress.config.ts; pkill -f 'vite preview'",
    "security:check": "npm audit --audit-level=high",
    "security:fix": "npm audit fix",
    "security:scan": "npm run security:check && npm run lint",
    "type-check": "tsc --noEmit --project config/typescript/tsconfig.json",
    "hooks:install": "pre-commit install --config config/quality/.pre-commit-config.yaml",
    "hooks:uninstall": "pre-commit uninstall",
    "hooks:run": "pre-commit run --all-files --config config/quality/.pre-commit-config.yaml",
    "validate:naming": "python3 scripts/01-validate-naming.py",
    "validate:extensions": "python3 scripts/02-validate-extensions.py",
    "validate:security": "python3 scripts/03-security-check.py $(find . -name '*.js' -o -name '*.jsx' -o -name '*.ts' -o -name '*.tsx' -o -name '*.py' | grep -v node_modules | grep -v .git)",
    "validate:todos": "python3 scripts/04-check-todos.py $(find . -name '*.js' -o -name '*.jsx' -o -name '*.ts' -o -name '*.tsx' -o -name '*.py' -o -name '*.md' -o -name '*.txt' | grep -v node_modules | grep -v .git)",
    "scan:vulnerabilities": "python3 scripts/05-vulnerability-scan.py",
    "monitor:dependencies": "python3 scripts/06-dependency-monitor.py",
    "audit:ci": "audit-ci --config config/quality/.audit-ci.json",
    "security:full": "npm run security:scan && npm run scan:vulnerabilities && npm run monitor:dependencies"
  },
